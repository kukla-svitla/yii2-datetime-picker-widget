{
    "name": "kukla-svitla/yii2-datetime-picker-widget",
    "description": "Date and time picker",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","picker","date","time"],
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Olexandr Kukla",
            "email": "o.kukla@svitla.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/datetimepicker": "@dev"
    },
    "autoload": {
        "psr-4": {
            "svitla\\dateTimePicker\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower-asset"
        }
    }
}
